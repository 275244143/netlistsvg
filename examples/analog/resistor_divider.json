{
  "modules": {
    "resistor_divider": {
      "ports": {
        "vcc": {
          "direction": "input",
          "bits": [ "vcc" ]
        },
        "vout": {
          "direction": "output",
          "bits": [ "vout" ]
        },
        "gnd": {
          "direction": "output",
          "bits": [ "gnd" ]
        }
      },
      "cells": {
        "r1": {
          "type": "r1",
          "port_directions": {
            "1": "input",
            "2": "output"
          },
          "connections": {
            "1" : [ "vcc" ],
            "2" : [ "vout" ]
          }
        },
        "r2": {
          "type": "r2",
          "port_directions": {
            "1": "input",
            "2": "output"
          },
          "connections": {
            "1" : [ "vout" ],
            "2" : [ "gnd" ]
          }
        }
      }
    }
  }
}
